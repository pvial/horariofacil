<h1>
  All dedicacions
</h1>

<a href="/dedicacions/new">
  Add a new dedicacion
</a>

<hr>

<table class="table">
  <tr>
    <th>
      ID
    </th>

    <th>
      Profesor
    </th>

    <th>
      Asignatura
    </th>

    <th>
      Dedicacion
    </th>

    <th>
      Created at
    </th>

    <th>
      Updated at
    </th>

    <th>
    </th>
  </tr>

  <% @dedicacions.each do |dedicacion| %>
  <tr>
    <td>
      <%= dedicacion.id %>
    </td>

    <td>
      <%= dedicacion.profesor_id %>
    </td>

    <td>
      <%= dedicacion.asignatura_id %>
    </td>

    <td>
      <%= dedicacion.dedicacion %>
    </td>

    <td>
      <%= time_ago_in_words(dedicacion.created_at) %> ago
    </td>

    <td>
      <%= time_ago_in_words(dedicacion.updated_at) %> ago
    </td>

    <td>
      <a href="/dedicacions/<%= dedicacion.id %>">
        Show details
      </a>
    </td>
  </tr>
  <% end %>
</table>
